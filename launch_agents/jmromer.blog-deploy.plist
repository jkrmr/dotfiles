<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>jmromer.blog-deploy</string>

    <key>ProgramArguments</key>
    <array>
      <string>/Users/jmromer/.bin/blog-deploy</string>
    </array>

    <key>Nice</key>
    <integer>1</integer>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Minute</key>
        <integer>30</integer>
    </dict>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/jmromer.blog-deploy.stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/jmromer.blog-deploy.stderr.log</string>
  </dict>
</plist>
